{
    "name": "leo/core-jwt",
    "description": "JSON Web Token Builder / Validator",
    "keywords": [
        "restful",
        "web service",
        "api",
        "json web token",
        "jwt"        
    ],
    "homepage": "https://github.com/tuankiet1708/core-jwt",
    "support": {
        "issues": "https://github.com/tuankiet1708/core-jwt/issues",
        "source": "https://github.com/tuankiet1708/core-jwt"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Leo Loves Coding",
            "email": "tuankiet1708@gmail.com",
            "homepage": "https://tranletuankiet.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "lcobucci/jwt": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Leo\\JWT\\": "src/"
        },
        "files": [
            "src/utilities/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
